<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:cc="http://creativecommons.org/ns#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:svg="http://www.w3.org/2000/svg"
   xmlns="http://www.w3.org/2000/svg"
   version="1.1"
   width="1280"
   height="720"
   id="svg2">
  <defs
     id="defs4" />
  <metadata
     id="metadata7">
    <rdf:RDF>
      <cc:Work
         rdf:about="">
        <dc:format>image/svg+xml</dc:format>
        <dc:type
           rdf:resource="http://purl.org/dc/dcmitype/StillImage" />
        <dc:title></dc:title>
      </cc:Work>
    </rdf:RDF>
  </metadata>
  <rect
     width="1280"
     height="720"
     x="0"
     y="0"
     id="rect0001"
     style="fill:{{ bgcolour }};stroke:none" />
  <g
     id="layer1">
{% for glyph in glyphs %}
    <text
       x="{{ glyph.x }}"
       y="{{ glyph.y }}"
       id="text{{ loop.index }}"
       xml:space="preserve"
       style="font-size:60px;font-style:normal;font-weight:normal;
              text-align:center;line-height:125%;letter-spacing:0px;
              word-spacing:0px;text-anchor:middle;fill:{{ fgcolour }};fill-opacity:1;
              stroke:none;font-family:Sans">
       <tspan
         x="{{ glyph.x }}"
         y="{{ glyph.y }}"
         id="tspan{{ loop.index}}">{{ glyph.glyph|e }}</tspan></text>
{% endfor%}
  </g>
</svg>
